{
  "name": "immersive-tv",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "tvos": "react-native run-ios --simulator \"Apple TV\" --scheme \"immersivetv-tvOS\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write ./**/*{.ts,.tsx,.js,.jsx} --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "npm:react-native-tvos@0.66.3-1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-video": "^5.2.0",
    "react-native-video-controls": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.24",
    "@types/react-native": "^0.63.17",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/react-native-video": "^5.0.12",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
